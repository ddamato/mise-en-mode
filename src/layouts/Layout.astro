---
import Head from '@components/Head.astro';
const { file } = Astro.props;
const clean = file && file.endsWith('md');
---

<!doctype html>
<html lang="en">
	<Head { ...Astro.props }/>
	<body data-clean={ clean }>
		<slot/>
		<img src="https://analytics.damato.design/api/track" style="display: none"/>
	</body>
</html>
